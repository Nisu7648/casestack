{
  "name": "casestack-backend",
  "version": "1.0.0",
  "description": "CASESTACK - Finalization & Defensibility System Backend",
  "main": "src/server.enhanced.js",
  "scripts": {
    "start": "node src/server.enhanced.js",
    "dev": "nodemon src/server.enhanced.js",
    "migrate": "npx prisma migrate deploy --schema=./prisma/schema.casestack.prisma",
    "migrate:dev": "npx prisma migrate dev --schema=./prisma/schema.casestack.prisma",
    "generate": "npx prisma generate --schema=./prisma/schema.casestack.prisma",
    "studio": "npx prisma studio --schema=./prisma/schema.casestack.prisma",
    "seed": "node prisma/seed.js",
    "backup": "bash scripts/backup.sh",
    "restore": "bash scripts/restore.sh",
    "logs": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "logs:audit": "tail -f logs/audit.log"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/s3-request-presigner": "^3.470.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.14.0",
    "archiver": "^6.0.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "prisma": "^5.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "casestack",
    "finalization",
    "defensibility",
    "audit",
    "consulting",
    "case-management"
  ],
  "author": "CASESTACK Team",
  "license": "PROPRIETARY"
}
